w.keras.layers import Conv2D, UpSampling2D\n","from tensorflow.keras.models import Model\n","from tensorflow.keras.optimizers import Adam\n","\n","# Set your input and output folders\n","input_folder = \"/content/drive/MyDrive/preprocessing_multiprocessingmodule\"\n","output_folder = \"/content/drive/MyDrive/segmentedimages_cmap\"\n","\n","# Create the output folder if it doesn't exist\n","os.makedirs(output_folder, exist_ok=True)\n","\n","# Load the pre-trained VGG16 model without top (classification) layers\n","base_model = VGG16(weights='imagenet', include_top=False, input_shape=(224, 224, 3))\n","\n","# Create a custom segmentation model\n","x = base_model.output\n","x = Conv2D(1, (1, 1), activation='sigmoid')(x)  # 1 channel for binary segmentation\n","x = UpSampling2D(size=(32, 32))(x)  # Upsampling to match the original image size\n","segmentation_model = Model(inputs=base_model.input, outputs=x)\n","\n","# Compile the segmentation model\n","segmentation_model.compile(optimizer=Adam(lr=1e-4), loss='binary_crossentropy', metrics=['accuracy'])\n","\n","# Define a colormap for visualization\n","cmap = ListedColormap(['black', 'green'])\n","\n","# List all preprocessed images\n","image_files = [os.path.join(input_folder, file) for file in os.listdir(input_folder) if file.endswith(\".jpg\")]\n","\n","# Loop through the images for segmentation\n","for image_path in image_files:\n","    # Load and preprocess the image\n","    img = cv2.imread(image_path)\n","    img = cv2.resize(img, (224, 224))  # Resize to match VGG16 input size\n","    img = img / 255.0  # Normalize the image to [0, 1]\n","\n","    # Perform segmentation\n","    seg_map = segmentation_model.predict(np.expand_dims(img, axis=0))[0]\n","\n","    # Threshold the segmentation map to obtain binary mask\n","    threshold = 0.5  # Adjust as needed\n","    mask = (seg_map \u003e threshold).astype(np.uint8)\n","\n","    # Apply colormap to the segmentation mask for visualization\n","    segmented_colormap = cmap(mask)\n","\n","    # Save the segmented mask with the same name as the preprocessed image\n","    output_filename = os.path.basename(image_path)\n","    output_path = os.path.join(output_folder, f\"seg_{output_filename}\")\n","    plt.imsave(output_path, segmented_colormap)\n","\n","    print(f\"Segmented: {output_filename}\")\n","\n","print(\"Segmentation complete.\")\n"]}],"metadata":{"colab":{"authorship_tag":"ABX9TyNDGR5WXQqsVfUYH6z8EDUi","name":"","version":""},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}